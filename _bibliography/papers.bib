  code = {},
  keywords = {Quantum Physics}
}



@article{xuTransverseVibrationsThin2015,
  abbr={EJP},
  title = {Transverse Vibrations of a Thin Loaded Rod: Theory and Experiment},
  shorttitle = {Transverse Vibrations of a Thin Loaded Rod},
  author = {Xu, Jue and Chen, Yuan-jie and Ma, Yong-li},
  date = {2015-09-01},
  journaltitle = {Eur. J. Phys.},
  volume = {36},
  number = {5},
  pages = {055035},
  issn = {0143-0807, 1361-6404},
  doi = {10.1088/0143-0807/36/5/055035},
  url = {https://iopscience.iop.org/article/10.1088/0143-0807/36/5/055035},
  abstract = {The general formulation of a determinate solution problem is deduced for the transverse vibrations of a thin loaded rod. The vibration frequencies of a thin homogeneous rod carrying a concentrated mass as a function of the load's position and mass are exactly solved. The dynamic measurement method of Young's modulus of the rods is presented within this theory. Our measurements of Young's modulus in the dynamic method agree with those in the traditional bending method, therefore the theory is verified by our experiments.},
  urldate = {2021-12-16}
}




