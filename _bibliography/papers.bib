
@article{xuLagrangianFormalismQuantum2021,
  abbr={arXiv},
  title = {On {{Lagrangian Formalism}} of {{Quantum Computation}}},
  author = {Xu, Jue},
  date = {2021-12-07},
  shortjournal = {ArXiv211204892 Quant-Ph},
  eprint = {2112.04892},
  eprinttype = {arxiv},
  primaryclass = {quant-ph},
  url = {http://arxiv.org/abs/2112.04892},
  urldate = {2021-12-16},
  abstract = {We reformulate quantum computation in terms of Lagrangian (sum-over-path) formalism, in contrast to the widely used Hamiltonian (unitary gate) formulation. We exemplify this formalism with some widely-studied models, including the standard quantum circuit model, quantum optimization heuristics, and quantum random walks. The meanings of Lagrangian (action) are interpreted in various contexts of quantum computation, such as complexity analysis. Furthermore, an analog quantum simulation scheme is suggested where the Lagrangian serves as the starting point and the sum-over-path method is applied.},
